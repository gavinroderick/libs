{
  "options": {
    "base": "./test/",
    "concurrency": 4
  },
  "tasks": [
    {
      "name": "job3",
      "operation": "sync",
      "src": "./test/folder/",
      "filter": "createUsers.yaml",
      "mutation": "mutation{createUsers(input:{listOfUsers: ${resource_list}, apiKey: ${apiKey} }) {regStatus, error{code, message} }}",
      "depth": 0,
      "prefix": "graphql",
      "metaData": {
        "cacheControl": "private, max-age=0, no-cache, must-revalidate, proxy-revalidate"
      }
    }
  ]
}
