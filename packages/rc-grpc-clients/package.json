{
  "name": "@restorecommerce/rc-grpc-clients",
  "version": "0.5.3",
  "description": "Restorecommerce GRPC service implementations",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@restorecommerce/grpc-client": "^0.4.2",
    "protobufjs": "^6.10.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@restorecommerce/protos": "^0.7.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.18.21",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "protoc": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.1",
    "ts-proto": "^1.115.4",
    "ts-proto-descriptors": "^1.6.0",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf -rf ./dist",
    "build:compile": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run build",
    "generate": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false --proto_path=../protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ../protos/io/restorecommerce/*.proto",
    "generate:google": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true,useExactTypes=false --proto_path=../protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ../protos/google/protobuf/*.proto"
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "./dist"
        ]
      }
    }
  },
  "gitHead": "0a6f7c94bfb1811c5dd60ab8c4912a41299b3717"
}
