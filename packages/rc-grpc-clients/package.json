{
  "name": "@restorecommerce/rc-grpc-clients",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/libs.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "grpc"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.1.7",
    "@grpc/proto-loader": "^0.5.5",
    "protobufjs": "^6.10.1",
    "rxjs": "^6.6.3",
    "@restorecommerce/grpc-client": "0.1.0"
  },
  "peerDependencies": {
    "graphql": "^15.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/core": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@restorecommerce/protos": "0.2.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.12.64",
    "graphql": "^15.4.0",
    "jest": "^26.6.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "ts-proto": "^1.38.0",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:compile",
    "build:clean": "rimraf -rf ./dist",
    "build:compile": "tsc -p tsconfig.lib.json",
    "test": "jest",
    "test:dev": "jest --watch",
    "prepublishOnly": "npm run build",
    "generate": "protoc --experimental_allow_proto3_optional --ts_proto_opt=outputClientImpl=false,env=node,useOptionals=true,outputJsonMethods=true,outputSchema=true --proto_path=./node_modules/@restorecommerce/protos --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated ./node_modules/@restorecommerce/protos/io/restorecommerce/*.proto"
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
